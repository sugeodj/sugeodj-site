#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules/webpack-bundle-analyzer/lib/bin/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules/webpack-bundle-analyzer/lib/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules/webpack-bundle-analyzer/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules/webpack-bundle-analyzer/lib/bin/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules/webpack-bundle-analyzer/lib/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules/webpack-bundle-analyzer/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/webpack-bundle-analyzer@4.7.0/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
else
  exec node  "$basedir/../webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
fi
